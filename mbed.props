<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <GccBinDir></GccBinDir>
    <GccIncludeDir></GccIncludeDir>
    <MakePath></MakePath>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="GccBinDir">
      <Value>$(GccBinDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GccIncludeDir">
      <Value>$(GccIncludeDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="MakePath">
      <Value>$(MakePath)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
  <ItemDefinitionGroup /> 
</Project>
